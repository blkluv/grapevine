{
  "name": "grapevine-api",
  "version": "0.1.10",
  "description": "Decentralized Data Feeds API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "api",
    "hono",
    "openapi",
    "postgresql",
    "web3"
  ],
  "author": "engineering@pinata.cloud",
  "license": "MIT",
  "dependencies": {
    "@coinbase/x402": "^0.7.1",
    "@hono/node-server": "^1.19.6",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.4",
    "dotenv": "^17.2.3",
    "hono": "^4.10.5",
    "ioredis": "^5.8.2",
    "pg": "^8.16.3",
    "pinata": "^2.5.1",
    "uuidv7": "^1.0.2",
    "viem": "^2.39.0",
    "x402": "^0.7.1",
    "x402-hono": "^0.7.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.3.14",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitest/coverage-v8": "^4.0.8",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}
