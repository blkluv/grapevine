{
  "name": "grapevine-client",
  "version": "0.1.5",
  "description": "Decentralized Data Feeds client",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "pnpm run --filter=grapevine-client generate:types && tsc",
    "start": "node dist/index.js",
    "generate:types": "NODE_ENV=test node generate-spec.mjs && openapi-typescript openapi.json -o src/types.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "api",
    "hono",
    "openapi",
    "postgresql",
    "web3"
  ],
  "author": "engineering@pinata.cloud",
  "license": "MIT",
  "dependencies": {
    "openapi-fetch": "^0.15.0",
    "openapi-typescript": "^7.10.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@hono/node-server": "^1.19.6",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.4",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/coverage-v8": "^4.0.8",
    "eslint": "^9.39.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}
